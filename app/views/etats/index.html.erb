<p id="notice"><%= notice %></p>

<h1>Etats</h1>

<table>
  <thead>
    <tr>
      <th>Etat</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @etats.each do |etat| %>
      <tr>
        <td><%= etat.etat %></td>
        <td><%= etat.description %></td>
        <td><%= link_to 'Show', etat %></td>
        <td><%= link_to 'Edit', edit_etat_path(etat) %></td>
        <td><%= link_to 'Destroy', etat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Etat', new_etat_path %>
